                                                    "id": "eb0e7c1b-7ae9-460f-a681-ac20de97662a",
                                                    "position": {
                                                        "x": -585.8560791015625,
                                                        "y": 36.422027587890628
                                                    },
                                                    "text": "This expression translates the Exponential-Logarithmic knob to a range of -1 to 1.",
                                                    "width": 256.0
                                                },
                                                {
                                                    "type": "Text",
                                                    "id": "d2f83e4c-96c8-4abe-8616-d1e923d3247e",
